{
  "manifest_version": 2,

  "name": "svg grabber",
  "description": "Grab all SVG's from a page",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "svggrabber48.png"
  },
  "icons": {
    "16": "svggrabber16.png",
    "48": "svggrabber48.png",
    "128": "svggrabber128.png"
  },
  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
